<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="BorderPaneConfigure" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlDocs.configure2">
   <top>
      <VBox fx:id="VBoxTopConfigure" alignment="TOP_CENTER" spacing="3.0" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="TextPriceConfigure" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Text>
            <Text fx:id="TextCSVConfigure" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
         </children>
         <BorderPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </BorderPane.margin>
      </VBox>
   </top>
   <bottom>
      <HBox fx:id="HBoxBottomConfigure" alignment="CENTER" spacing="50.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </BorderPane.margin>
         <children>
            <Button cancelButton="true" defaultButton="true" minWidth="150.0" mnemonicParsing="false" onMouseClicked="#compare" text="Compare" />
            <Button cancelButton="true" defaultButton="true" minWidth="150.0" mnemonicParsing="false" onMouseClicked="#addToBag" text="Add To Bag" />
            <Button cancelButton="true" defaultButton="true" minWidth="150.0" mnemonicParsing="false" onMouseClicked="#reviewBag" text="Review Bag" />
         </children>
      </HBox>
   </bottom>
   <center>
      <HBox fx:id="HBoxConfigureMain" alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <ScrollPane fx:id="ScrollPaneButtons" hbarPolicy="NEVER">
               <content>
                  <VBox fx:id="VBoxButtons" alignment="TOP_CENTER" spacing="10.0">
                     <padding>
                        <Insets left="20.0" right="20.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
      </HBox>
   </center>
</BorderPane>
